<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.bjgreenchem.production.cost.dao.StudentDao">  
<select id="getStudent" parameterType="com.bjgreenchem.production.cost.model.Student" resultType="com.bjgreenchem.production.cost.model.Student">  
    SELECT stud_id studId, name name, dob, phone FROM students WHERE stud_id=#{studId}
</select>  
</mapper>